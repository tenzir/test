---
timeout: 60
fixtures: [node]
error: false
---

pipeline::detach {
  every 10ms {
    from {id: 1}, {id: 2}
  }
  publish "foo"
}

pipeline::detach {
  subscribe "foo"
  where id == 1
  publish "bar"
}

pipeline::detach {
  subscribe "foo"
  where id == 2
  publish "bar"
}

subscribe "bar"
deduplicate
head 2
sort
